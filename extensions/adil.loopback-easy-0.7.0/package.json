{
	"name": "loopback-easy",
	"displayName": "Loopback Code Easy",
	"description": "A loopback extension",
	"version": "0.7.0",
	"engines": {
		"vscode": "^1.33.0"
	},
	"categories": [
		"Other",
		"Snippets"
	],
	"activationEvents": [
		"onCommand:extension.createRemoteMethod",
		"onCommand:extension.createRemoteHook"
	],
	"main": "./extension.js",
	"contributes": {
		"commands": [
			{
				"command": "extension.createRemoteMethod",
				"title": "Create Remote Method"
			},
			{
				"command": "extension.createRemoteHook",
				"title": "Create Remote Hook"
			}
		],
		"snippets": [
			{
				"language": "javascript",
				"path": "./snippets/snippets.json"
			}
		]
	},
	"scripts": {
		"postinstall": "node ./node_modules/vscode/bin/install",
		"test": "node ./node_modules/vscode/bin/test"
	},
	"devDependencies": {
		"typescript": "^3.3.1",
		"vscode": "^1.1.28",
		"eslint": "^5.13.0",
		"@types/node": "^10.12.21",
		"@types/mocha": "^2.2.42"
	},
